{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:solid/typescript"
    ],
    "overrides": [
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "solid"
    ],
    "rules": {
        "semi": "warn",
        "quotes": ["warn", "single"],
        "no-unused-vars": "off"
    }
}
